<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="ru.samsung.itschool.musicclient">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/it_academy_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/it_academy_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MusicClient"
        tools:targetApi="m">
        <activity android:name="ru.samsung.itschool.musicclient.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>